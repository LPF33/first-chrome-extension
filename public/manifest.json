{
    "name": "Multi Chrome Extension",
    "description": "Build first extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Open Multitool",
        "default_icon": {
            "16": "/assets/icon.gif",
            "32": "/assets/icon.gif",
            "48": "/assets/icon.gif",
            "128": "/assets/icon.gif"
        }
    },
    "icons": {
        "16": "/assets/icon.gif",
        "32": "/assets/icon.gif",
        "48": "/assets/icon.gif",
        "128": "/assets/icon.gif"
    }
}
